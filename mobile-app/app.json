{
  "expo": {
    "name": "My Med Buddy",
    "slug": "mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logo.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/img/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5FA"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#F5F5FA",
      "bundleIdentifier": "com.feikfeik10.mobileapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logo.jpg",
        "backgroundColor": "#F5F5FA"
      },
      "package": "com.feikfeik10.mobileapp"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/exo-2/Exo2_100Thin.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_200ExtraLight.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_300Light.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_400Regular.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_500Medium.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_700Bold.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_800ExtraBold.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_900Black.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_100Thin_Italic.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_200ExtraLight_Italic.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_300Light_Italic.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_400Regular_Italic.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_500Medium_Italic.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_600SemiBold_Italic.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_700Bold_Italic.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_800ExtraBold_Italic.ttf",
            "node_modules/@expo-google-fonts/exo-2/Exo2_900Black_Italic.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9952e12c-91ba-467b-a65f-ef2491627852"
      }
    }
  }
}