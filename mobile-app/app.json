{
  "expo": {
    "name": "mobile-app",
    "slug": "mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logo.svg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/img/logo.svg",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5FA"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#F5F5FA"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logo.svg",
        "backgroundColor": "#F5F5FA"
      }
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/exo-2/Exo2_100Thin.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_200ExtraLight.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_300Light.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_400Regular.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_500Medium.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_600SemiBold.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_700Bold.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_800ExtraBold.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_900Black.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_100Thin_Italic.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_200ExtraLight_Italic.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_300Light_Italic.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_400Regular_Italic.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_500Medium_Italic.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_600SemiBold_Italic.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_700Bold_Italic.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_800ExtraBold_Italic.tff",
            "node_modules/@expo-google-fonts/exo-2/Exo2_900Black_Italic.tff"
          ]
        }
      ]
    ]
  }
}
